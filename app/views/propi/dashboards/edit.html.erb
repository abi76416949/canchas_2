<h1>Editar Datos del Propietario</h1>

<%= form_with model: @propietario, url: propi_update_dashboard_path, method: :patch do |f| %>
  <div class="form-group">
    <%= f.label :nombre %>
    <%= f.text_field :nombre, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :direccion %>
    <%= f.text_field :direccion, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :contacto %>
    <%= f.text_field :contacto, class: "form-control" %>
  </div>

  <%= f.submit "Guardar cambios", class: "btn btn-success" %>
<% end %>
